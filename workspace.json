{
  "version": 2,
  "projects": {
    "mobile-shell-feature": {
      "projectType": "library",
      "root": "libs/mobile/shell/feature",
      "sourceRoot": "libs/mobile/shell/feature/src",
      "prefix": "food",
      "targets": {
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/mobile/shell/feature"],
          "options": {
            "jestConfig": "libs/mobile/shell/feature/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/mobile/shell/feature/src/**/*.ts", "libs/mobile/shell/feature/src/**/*.html"]
          }
        }
      }
    },
    "tacos-api": {
      "root": "apps/tacos-api",
      "sourceRoot": "apps/tacos-api/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/tacos-api",
            "main": "apps/tacos-api/src/main.ts",
            "tsConfig": "apps/tacos-api/tsconfig.app.json",
            "assets": ["apps/tacos-api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/tacos-api/src/environments/environment.ts",
                  "with": "apps/tacos-api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "tacos-api:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/tacos-api/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/tacos-api"],
          "options": {
            "jestConfig": "apps/tacos-api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "tacos-mobile": {
      "projectType": "application",
      "root": "apps/tacos-mobile",
      "sourceRoot": "apps/tacos-mobile/src",
      "prefix": "food",
      "targets": {
        "build": {
          "executor": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/tacos-mobile",
            "index": "apps/tacos-mobile/src/index.html",
            "main": "apps/tacos-mobile/src/main.ts",
            "polyfills": "apps/tacos-mobile/src/polyfills.ts",
            "tsConfig": "apps/tacos-mobile/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/tacos-mobile/src/assets",
              {
                "glob": "**/*.svg",
                "input": "node_modules/ionicons/dist/ionicons/svg",
                "output": "./svg"
              }
            ],
            "styles": [
              "apps/tacos-mobile/src/styles.scss",
              {
                "input": "apps/tacos-mobile/src/theme/variables.scss"
              }
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/tacos-mobile/src/environments/environment.ts",
                  "with": "apps/tacos-mobile/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "executor": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "tacos-mobile:build:production"
            },
            "development": {
              "browserTarget": "tacos-mobile:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "tacos-mobile:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/tacos-mobile/src/**/*.ts", "apps/tacos-mobile/src/**/*.html"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/tacos-mobile"],
          "options": {
            "jestConfig": "apps/tacos-mobile/jest.config.js",
            "passWithNoTests": true
          }
        },
        "cap": {
          "executor": "@nxtend/capacitor:cap",
          "options": {
            "cmd": "--help"
          }
        },
        "add": {
          "executor": "@nxtend/capacitor:cap",
          "options": {
            "cmd": "add"
          },
          "configurations": {
            "ios": {
              "cmd": "add ios"
            },
            "android": {
              "cmd": "add android"
            }
          }
        },
        "copy": {
          "executor": "@nxtend/capacitor:cap",
          "options": {
            "cmd": "copy"
          },
          "configurations": {
            "ios": {
              "cmd": "copy ios"
            },
            "android": {
              "cmd": "copy android"
            }
          }
        },
        "open": {
          "executor": "@nxtend/capacitor:cap",
          "options": {
            "cmd": "open"
          },
          "configurations": {
            "ios": {
              "cmd": "open ios"
            },
            "android": {
              "cmd": "open android"
            }
          }
        },
        "run": {
          "executor": "@nxtend/capacitor:cap",
          "options": {
            "cmd": "run"
          },
          "configurations": {
            "ios": {
              "cmd": "run ios"
            },
            "android": {
              "cmd": "run android"
            }
          }
        },
        "sync": {
          "executor": "@nxtend/capacitor:cap",
          "options": {
            "cmd": "sync"
          },
          "configurations": {
            "ios": {
              "cmd": "sync ios"
            },
            "android": {
              "cmd": "sync android"
            }
          }
        },
        "update": {
          "executor": "@nxtend/capacitor:cap",
          "options": {
            "cmd": "update"
          },
          "configurations": {
            "ios": {
              "cmd": "update ios"
            },
            "android": {
              "cmd": "update android"
            }
          }
        }
      }
    },
    "tacos-mobile-e2e": {
      "root": "apps/tacos-mobile-e2e",
      "sourceRoot": "apps/tacos-mobile-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/tacos-mobile-e2e/cypress.json",
            "devServerTarget": "tacos-mobile:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "tacos-mobile:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/tacos-mobile-e2e/**/*.{js,ts}"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "tacos-mobile",
  "generators": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  }
}
